<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/item_list_fragment">

    <fragment
        android:id="@+id/item_list_fragment"
        android:name="com.amlavati.digikraft.ui.two.BikeStationsListFragment"
        android:label="@string/app_name">
        <action
            android:id="@+id/show_item_detail"
            app:destination="@id/item_detail_fragment" />
    </fragment>
    <fragment
        android:id="@+id/item_detail_fragment"
        android:name="com.amlavati.digikraft.ui.two.BikeStationsDetailFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_item_detail">
        <argument
            android:name="item_data"
            app:argType="com.amlavati.digikraft.model.Feature" />
    </fragment>
</navigation>